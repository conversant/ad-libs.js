{
  "name": "adlibs",
  "version": "2.5.0",
  "description": "A collection of common client side commonjs modules",
  "scripts": {
    "test": "karma start --single-run",
    "test-coverall": "karma start --single-run --with-coverage && cat ./coverage/*/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "doc": "jsdoc2md -t doc/README.hbs 'lib/**/*.js' > README.md; echo"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/conversant/adlibs.git"
  },
  "keywords": [
    "util",
    "modules",
    "cross-browser"
  ],
  "author": "Conversant Media",
  "license": "Apache-2.0",
  "dependencies": {},
  "devDependencies": {
    "coveralls": "^3.0.4",
    "expect.js": "^0.3.1",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jsdoc-to-markdown": "^5.0.0",
    "jshint": "^2.10.2",
    "karma": "^4.1.0",
    "karma-bamboo-reporter": "^0.1.2",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "^1.0.1",
    "karma-coverage": "^1.1.2",
    "karma-firefox-launcher": "^1.1.0",
    "karma-mocha": "^1.3.0",
    "karma-safari-launcher": "^1.0.0",
    "karma-safarinative-launcher": "^1.0.0",
    "karma-sauce-launcher": "^2.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-verbose-summary-reporter": "0.0.1",
    "karma-webpack": "^2.0.13",
    "mocha": "^6.1.4",
    "node-libs-browser": "^2.2.1",
    "request": "^2.88.0",
    "sinon": "^7.3.2",
    "webpack": "^4.34.0",
    "yargs": "^11.0.0"
  },
  "bugs": {
    "url": "https://github.com/conversant/adlibs/issues"
  },
  "homepage": "https://github.com/conversant/adlibs",
  "directories": {
    "doc": "doc",
    "test": "test"
